{
  "author": "Taiga Ebihara",
  "background": ["background.js"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["main.js"],
      "css": ["main.css"]
    }
  ],
  "name": "My Extension",
  "manifest_version": 3,
  "version": "0.0.1",
  "web_accessible_resources": [
    {
      "resources": ["assets/*"],
      "matches": ["<all_urls>"]
    }
  ]
}
